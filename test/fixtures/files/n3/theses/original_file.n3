@prefix ebucore: <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#> .
@prefix fcrepo4: <http://fedora.info/definitions/v4/repository#> .
@prefix pcdm: <http://pcdm.org/models#> .
@prefix premis: <http://www.loc.gov/premis/rdf/v1#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<<%= url %>/aip/v1/theses/<%= entity_id %>/filesets/<%= fileset_id %>/original_file> a pcdm:File;
  fcrepo4:hasFixityService <<%= url %>/aip/v1/theses/<%= entity_id %>/filesets/<%= fileset_id %>/fixity>;
  fcrepo4:hasParent <<%= url %>/aip/v1/theses/<%= entity_id %>/filesets/<%= fileset_id %>/>;
  ebucore:hasMimeType "image/jpeg";
  ebucore:resourceFilename "<%= filename %>";
  premis:hasMessageDigest <urn:md5:<%= checksum %>>;
  premis:hasSize <%= byte_size %> .
